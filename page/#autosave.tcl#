        -borderwidth 2 -relief groove -width 415 -height 405 
    vTcl:DefineAlias "$top.tFr49" "TFrame1" vTcl:WidgetProc "login" 1
### SPOT dump_widget_opt A
    set site_3_0 $top.tFr49
